<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation">

    <fragment
        android:id="@+id/navigation_new_habit"
        tools:layout="@layout/fragment_create_habit"
        android:name="com.tjcg.habitapp.fragment.CreateNewHabitFragment"
        android:label="CreateNewHabitFragment" >
        <action
            android:id="@+id/action_navigation_new_habit_to_iconListFragment"
            app:destination="@id/iconListFragment" />
    </fragment>

    <fragment android:id="@+id/navigation_journey_desc"
        android:name="com.tjcg.habitapp.fragment.JourneyDescriptionFragment"
        tools:layout="@layout/fragment_journey_description"
        android:label="Journey Description">
        <action
            android:id="@+id/action_navigation_journey_desc_to_navigation_premium"
            app:destination="@id/navigation_premium" />
    </fragment>

    <fragment android:id="@+id/navigation_premium"
        android:name="com.tjcg.habitapp.fragment.GoPremiumFragment"
        tools:layout="@layout/fragment_go_premium"
        android:label="Go Premium"/>

    <fragment android:id="@+id/navigation_history"
        android:name="com.tjcg.habitapp.fragment.HistoryMainFragment"
        tools:layout="@layout/fragment_history_main"
        android:label="History"/>

    <fragment android:id="@+id/navigation_profile_g_settings"
        android:name="com.tjcg.habitapp.fragment.MyProfileGeneralSettingsFragment"
        tools:layout="@layout/fragment_my_profile_general_settings"
        android:label="General Settings">
        <action
            android:id="@+id/action_navigation_profile_g_settings_to_navigation_privacy_policy"
            app:destination="@id/navigation_privacy_policy" />
        <action
            android:id="@+id/action_navigation_profile_g_settings_to_myProfileTimePeriodSettings"
            app:destination="@id/myProfileTimePeriodSettings" />
    </fragment>

    <fragment android:id="@+id/navigation_privacy_policy"
        android:name="com.tjcg.habitapp.fragment.PrivacyPolicyFragment"
        tools:layout="@layout/fragment_privacy_policy"
        android:label="Privacy Policy"/>
    <fragment
        android:id="@+id/rateUsFragment"
        tools:layout="@layout/fragment_rate_us"
        android:name="com.tjcg.habitapp.fragment.RateUsFragment"
        android:label="RateUsFragment" />
    <fragment
        android:id="@+id/feedbackFragment"
        android:name="com.tjcg.habitapp.fragment.FeedbackFragment"
        tools:layout="@layout/fragment_feedback"
        android:label="FeedbackFragment" />
    <fragment
        android:id="@+id/habitPresetsFragment"
        android:name="com.tjcg.habitapp.fragment.HabitPresetsFragment"
        tools:layout="@layout/fragment_habit_presets"
        android:label="HabitPresetsFragment" >
        <action
            android:id="@+id/action_habitPresetsFragment_to_navigation_new_habit"
            app:destination="@id/navigation_new_habit" />
        <action
            android:id="@+id/action_habitPresetsFragment_to_habitsInPresetFragment"
            app:destination="@id/habitsInPresetFragment" />
    </fragment>
    <fragment
        android:id="@+id/timerFragment"
        android:name="com.tjcg.habitapp.fragment.TimerFragment"
        tools:layout="@layout/fragment_timer"
        android:label="TimerFragment" />
    <fragment
        android:id="@+id/iconListFragment"
        android:name="com.tjcg.habitapp.fragment.IconListFragment"
        tools:layout="@layout/fragment_icon_select"
        android:label="IconListFragment" />
    <fragment
        android:id="@+id/myProfileNotificationFragment"
        android:name="com.tjcg.habitapp.fragment.MyProfileNotificationFragment"
        tools:layout="@layout/fragment_my_profile_notification"
        android:label="MyProfileNotificationFragment" />
    <fragment
        android:id="@+id/myProfileTimePeriodSettings"
        android:name="com.tjcg.habitapp.fragment.MyProfileTimePeriodSettings"
        tools:layout="@layout/fragment_my_profile_time_periods"
        android:label="MyProfileTimePeriodSettings" />
    <fragment
        android:id="@+id/habitsInPresetFragment"
        android:name="com.tjcg.habitapp.fragment.HabitsInPresetFragment"
        tools:layout="@layout/fragment_preset_habits"
        android:label="HabitsInPresetFragment" />
</navigation>